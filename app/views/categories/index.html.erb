<h1 align="center">Listing all categories</h1>

<div align="center">
  <div class="pagination">
    <%= will_paginate%>
  </div>
<% @categories.each do |category| %>
  <ul class="listing">
    <div class="row">
      <div class="card col-md-4 offset-md-4">
        <li class="article-title">
          <%= link_to "#{category.name}", category_path(category.id) %>
        </li>
      </div>
    </div>
  </ul>
<%end%>
</div>
